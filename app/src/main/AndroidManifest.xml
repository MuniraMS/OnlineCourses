<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.assignment">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="online course application"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.studentsinformation"></activity>
        <activity android:name=".activities.web$studentsinfo" />
        <activity android:name=".activities.logicaldesign" />
        <activity android:name=".activities.web" />
        <activity android:name=".activities.graphic" />
        <activity android:name=".activities.coursesinfo" />
        <activity android:name=".activities.Signupstudent" />
        <activity android:name=".activities.Signupteacher" />
        <activity android:name=".activities.student" />
        <activity android:name=".activities.teacher" />
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>